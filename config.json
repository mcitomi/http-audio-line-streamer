{
    "http_port": 8080,
    "stream": {
        "ws_enabled" : true,
        "http_enabled" : true,
        "audio_line": "CABLE Output (VB-Audio Virtual Cable)",
        "audio_api": "dshow",
        "codec": "libmp3lame",
        "bitrate": 320,
        "format": "mp3"
    },
    "monitoring": {
        "refresh_interval": 45,
        "enable_ffmpeg_log": true,
        "ffmpeg_log_length": 50,
        "screen_max_scrollback": 1000,
        "use_ps_fetch": false,
        "save_main_log" : true,
        "save_ffmpeg_log" : false,
        "save_resource_log" : false
    }
}